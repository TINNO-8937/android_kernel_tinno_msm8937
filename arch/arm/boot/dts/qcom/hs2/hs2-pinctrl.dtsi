&soc {
	tlmm: pinctrl@1000000 {
		tlmm_gpio_fpid {
			gpio_fpid_active: gpio_fpid_active {
				mux {
					pins = "gpio16";
					function = "gpio";
				};

				config {
					pins = "gpio16";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};
	};
};
